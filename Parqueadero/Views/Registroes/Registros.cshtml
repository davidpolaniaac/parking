@model IEnumerable<Parqueadero.Models.Registro>

@{
    ViewBag.Title = "Registros";
}

<h2>Registros</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Placa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaEntrada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaSalida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostoFinal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Servicio.NombreServcio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Placa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaSalida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostoFinal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Servicio.NombreServcio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdRegistro }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdRegistro }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdRegistro })
        </td>
    </tr>
}

</table>
